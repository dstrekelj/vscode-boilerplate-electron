{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch",
            "type": "node",
            "cwd": "${workspaceRoot}",
            "program": "${workspaceRoot}/src/index.js",
            "runtimeExecutable": "${workspaceRoot}/node_modules/.bin/electron.cmd",
            "runtimeArgs": [
                "--enable-logging",
                "--remote-debugging-port=9222"
            ]
        }
    ]
}